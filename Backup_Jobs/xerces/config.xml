  <builders>
    <hudson.plugins.xshell.XShellBuilder plugin="xshell@0.10">
      <commandLine>Environ.bat</commandLine>
      <executeFromWorkingDir>true</executeFromWorkingDir>
      <regexToKill></regexToKill>
      <timeAllocated></timeAllocated>
    </hudson.plugins.xshell.XShellBuilder>
    <EnvInjectBuilder plugin="envinject@1.92.1">
      <info>
        <propertiesFilePath>$WORKSPACE\Environ.props</propertiesFilePath>
      </info>
    </EnvInjectBuilder>
    <hudson.tasks.BatchFile>
      <command>@echo off&#xd;
setlocal&#xd;
&#xd;
echo [INFO]MSBuildToolsPath: %MSBuildToolsPath%&#xd;
echo [INFO]SEVENZ: %SEVENZ%&#xd;
&#xd;
::-Extract the appropriate Project files only for VS 2012 and above&#xd;
if NOT &quot;%T%&quot;==&quot;v100&quot; (&#xd;
	echo [INFO]Calling: %SEVENZ% x -aoa xerces\%VCNAME_ALT%.7z -o&quot;%WORKSPACE%\projects\Win32&quot;&#xd;
	%SEVENZ% x -aoa xerces\%VCNAME_ALT%.7z -o&quot;%WORKSPACE%\projects\Win32&quot;&#xd;
	if ERRORLEVEL 1 (&#xd;
		echo [FAILURE]: Extraction of xerces\%VCNAME_ALT%.7z failed.&#xd;
		exit /b ERRORLEVEL&#xd;
	)&#xd;
)&#xd;
&#xd;
set SLN=%WORKSPACE%\projects\Win32\%VCNAME_ALT%\xerces-all\xerces-all.sln&#xd;
&#xd;
echo [INFO]Calling: %MSBUILD% /m /t:build /p:Configuration=&quot;%C%&quot; /p:Platform=%P% /p:PlatformToolset=%T% &quot;%SLN%&quot;&#xd;
%MSBUILD% /m /t:build /p:Configuration=&quot;%C%&quot; /p:Platform=%P% /p:PlatformToolset=%T% &quot;%SLN%&quot;&#xd;
if ERRORLEVEL 1 (&#xd;
	echo [FAILURE]: Failed to build %SLN% using %MSBUILD%&#xd;
	exit /b ERRORLEVEL&#xd;
)&#xd;
&#xd;
::-Success archive the artifacts&#xd;
set XERCESC_BIN_ARCHIVE=xerces-%VERSION%-bin-%C%-%P%-%T%.7z&#xd;
echo [INFO]Calling: %SEVENZ% a -t7z &quot;%XERCESC_BIN_ARCHIVE%&quot; -i!&quot;Build\*\*\%C%\xerces-c*.*&quot; -xr!*.exp -xr!*.ilk&#xd;
%SEVENZ% a -t7z &quot;%XERCESC_BIN_ARCHIVE%&quot; -i!&quot;Build\*\*\%C%\xerces-c*.*&quot; -xr!*.exp -xr!*.ilk&#xd;
if ERRORLEVEL 1 (&#xd;
	echo [FAILURE]: Failed to create archive %XERCESC_BIN_ARCHIVE%&#xd;
	exit /b ERRORLEVEL&#xd;
)&#xd;
&#xd;
if &quot;%T%&quot;==&quot;v120&quot; (&#xd;
	if &quot;%P%&quot;==&quot;x64&quot; (&#xd;
		if &quot;%C%&quot;==&quot;Release&quot; (&#xd;
			echo [INFO]Toolkit=%T%, Platform=%P%,Configuration=%C% archiving the Xerces headers&#xd;
			&#xd;
			echo [INFO]Calling: %SEVENZ% a -t7z &quot;%WORKSPACE%\xerces-%VERSION%-inc.7z&quot; -ir!src -xr!.svn -xr!tests -xr!samples&#xd;
			%SEVENZ% a -t7z &quot;%WORKSPACE%\xerces-%VERSION%-inc.7z&quot; -ir!src -xr!.svn -xr!tests -xr!samples&#xd;
			&#xd;
			echo [INFO]Calling: %SEVENZ% a -t7z &quot;xerces-%VERSION%-license.7z&quot; -i!LICENSE&#xd;
			%SEVENZ% a -t7z &quot;xerces-%VERSION%-license.7z&quot; -i!LICENSE&#xd;
		) else (&#xd;
			echo [INFO]Toolkit=%T%, Platform=%P%,Configuration=%C% hence skipping archiving the Xerces-C headers&#xd;
		)&#xd;
	) else (&#xd;
		echo [INFO]Toolkit=%T%, Platform=%P% hence skipping archiving the Xerces-C headers&#xd;
	)&#xd;
) else (&#xd;
	echo [INFO]Toolkit is %T%, hence skipping archiving the Xalan-C headers&#xd;
)&#xd;
::-Success archive the xerces sources&#xd;
&#xd;
&#xd;
endlocal&#xd;
&#xd;
</command>
    </hudson.tasks.BatchFile>

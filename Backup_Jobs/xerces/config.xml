    <EnvInjectJobProperty plugin="envinject@1.92.1">
      <info>
        <groovyScriptContent>// Copyright (c) 2014 Advanced Micro Devices, Inc. All rights reserved.

import java.lang.String;

def map = [:] ;

if ( JOB_NAME.contains( &quot;v120&quot; ) ) {
	map.put( &quot;VSNAME&quot;, &quot;VS2013&quot; );
	map.put( &quot;VCNAME_FULL&quot;, &quot;Visual C++ 2013&quot; );
	map.put( &quot;VCTOOLKIT_SHORT&quot;, &quot;vc12&quot; );
	map.put( &quot;VCNAME_ALT&quot;, &quot;VC12&quot; );
}

if ( JOB_NAME.contains( &quot;v110&quot; ) ) {
	map.put( &quot;VSNAME&quot;, &quot;VS2012&quot; );
	map.put( &quot;VCNAME_FULL&quot;, &quot;Visual C++ 2012&quot; );
	map.put( &quot;VCTOOLKIT_SHORT&quot;, &quot;vc11&quot; );
	map.put( &quot;VCNAME_ALT&quot;, &quot;VC11&quot; );
}

if ( JOB_NAME.contains( &quot;v100&quot; ) ) {
	map.put( &quot;VSNAME&quot;, &quot;VS2010&quot; );
	map.put( &quot;VCNAME_FULL&quot;, &quot;Visual C++ 2010&quot; );
	map.put( &quot;VCTOOLKIT_SHORT&quot;, &quot;vc10&quot; );
	map.put( &quot;VCNAME_ALT&quot;, &quot;VC10&quot; );
}

map.put( &quot;J&quot;, &quot;1.8&quot; );

println &apos;---------------------------------&apos;;
println map;
println &apos;---------------------------------&apos;;

return map;</groovyScriptContent>
        <loadFilesFromMaster>false</loadFilesFromMaster>
      </info>

      <info>
        <groovyScriptContent>// ****************************************************************************************************
// Copyright (c) 2014 Advanced Micro Devices, Inc. All rights reserved.

import java.lang.String;

println &apos;JOB_NAME: &apos; + JOB_NAME;
println &apos;BUILD_MATRIX: &apos; + BUILD_MATRIX;

//Dump the environment variables
def map = [:];

def ARCHIVE_EXTENSION = &quot;7z&quot;
map.put( &quot;ARCHIVE_EXTENSION&quot;, ARCHIVE_EXTENSION );

def parPRODUCTVERSION;
try {
	parPRODUCTVERSION = PRODUCTVERSION;
	println &apos;PRODUCTVERSION found: &apos; + PRODUCTVERSION;
}
catch( groovy.lang.MissingPropertyException e ) {
	//PRODUCTVERSION not present
	println &apos;PRODUCTVERSION NOT found&apos;;
	parPRODUCTVERSION = MAJOR_NUMBER + &quot;.&quot; + MINOR_NUMBER 
	if ( &quot;0&quot; != SUBMINOR_NUMBER ) {
		parPRODUCTVERSION += &quot;-&quot; + SUBMINOR_NUMBER;
	}

	parPRODUCTVERSION += &quot;.&quot; + BUILD_NUMBER;
	map.put( &quot;PRODUCTVERSION&quot;, parPRODUCTVERSION );
}

def parVERSION;
try {
	parVERSION = VERSION;
	println &apos;VERSION found: &apos; + VERSION;
}
catch( groovy.lang.MissingPropertyException e ) {
	//VERSION not present
	println &apos;VERSION NOT found&apos;;
	parVERSION = parPRODUCTVERSION + &quot;.0&quot;;
	map.put( &quot;VERSION&quot;, parVERSION );	
}

def parARTIFACT_PRODUCTVERSION;
try {
	parARTIFACT_PRODUCTVERSION = ARTIFACT_PRODUCTVERSION;
	println &apos;ARTIFACT_PRODUCTVERSION found: &apos; + ARTIFACT_PRODUCTVERSION;
}
catch( groovy.lang.MissingPropertyException e ) {
	//ARTIFACT_PRODUCT not present
	println &apos;ARTIFACT_PRODUCTVERSION NOT found&apos;;
	parARTIFACT_PRODUCTVERSION = MAJOR_NUMBER + &quot;.&quot; + MINOR_NUMBER;
	if ( &quot;0&quot; != SUBMINOR_NUMBER ) {
		parARTIFACT_PRODUCTVERSION += &quot;-&quot; + SUBMINOR_NUMBER;
	}
	parARTIFACT_PRODUCTVERSION +=  &quot;.&quot; + BUILD_NUMBER + &quot;-&quot; + MILESTONE;
	
	map.put( &quot;ARTIFACT_PRODUCTVERSION&quot;, parARTIFACT_PRODUCTVERSION );	
}

if( JOB_NAME.contains( &quot;Win32&quot; ) ) {
	map.put( &quot;PLATFORM_ARCH_SHORT&quot;, &quot;x86&quot; );
	map.put( &quot;PLATFORM_ARCH_INTEL&quot;, &quot;ia32&quot; );
	map.put( &quot;PLATFORM_ARCH_INTEL_ALT&quot;, &quot;x86&quot; );
	map.put( &quot;PLATFORM_ARCH_AMD&quot;, &quot;x86&quot; );
	map.put( &quot;ARCH&quot;, &quot;32&quot; );
	map.put( &quot;PLATFORM_ARCH_WIN&quot;, &quot;win32&quot; );
}

if( JOB_NAME.contains( &quot;x64&quot; ) ) {
	map.put( &quot;PLATFORM_ARCH_SHORT&quot;, &quot;x64&quot; );
	map.put( &quot;PLATFORM_ARCH_INTEL&quot;, &quot;intel64&quot; );
	map.put( &quot;PLATFORM_ARCH_INTEL_ALT&quot;, &quot;x86_64&quot; );
	map.put( &quot;PLATFORM_ARCH_AMD&quot;, &quot;amd64&quot; );
	map.put( &quot;ARCH&quot;, &quot;64&quot; );
	map.put( &quot;PLATFORM_ARCH_WIN&quot;, &quot;win64&quot; );
}

String strVSName = &quot;VS2013&quot;;
String strVSNameSmall = &quot;vs13&quot;;
String strVSNameCaps = &quot;VS13&quot;;

if ( JOB_NAME.contains( &quot;v120&quot; ) ) {
	strVSName = &quot;VS2013&quot;;
	strVSNameSmall = &quot;vs12&quot;;
	strVSNameCaps = &quot;VS12&quot;;
	map.put( &quot;VCTOOLKIT_SHORT&quot;, &quot;vc12&quot; );
	map.put( &quot;VSKEY&quot;, &quot;12.0&quot; );
}

if ( JOB_NAME.contains( &quot;v110&quot; ) ) {
	strVSName = &quot;VS2012&quot;;
	strVSNameSmall = &quot;vs12&quot;;
	strVSNameCaps = &quot;VS12&quot;;
	map.put( &quot;VCTOOLKIT_SHORT&quot;, &quot;vc11&quot; );
	map.put( &quot;VSKEY&quot;, &quot;11.0&quot; );
}

if ( JOB_NAME.contains( &quot;v100&quot; ) ) {
	strVSName = &quot;VS2010&quot;;
	strVSNameSmall = &quot;vs10&quot;;	
	strVSNameCaps = &quot;VS10&quot;;
	map.put( &quot;VCTOOLKIT_SHORT&quot;, &quot;vc10&quot; );
	map.put( &quot;VSKEY&quot;, &quot;10.0&quot; );
}

map.put( &quot;VSNAME_SMALL&quot;, strVSNameSmall );
map.put( &quot;VSNAME_CAPS&quot;, strVSNameCaps );
map.put( &quot;VSNAME&quot;, strVSName );

map.put( &quot;J&quot;, &quot;1.8&quot; );

println &apos;--------------------------------------------------------------&apos;;
println &apos;Environment variables inserted: &apos; + map;
println &apos;--------------------------------------------------------------&apos;;

return map;</groovyScriptContent>

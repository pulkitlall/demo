    <hudson.tasks.BatchFile>
      <command>@echo off&#xd;
&#xd;
setlocal&#xd;
&#xd;
echo COPYARTIFACT_BUILD_NUMBER_MEDIASDK_BUILD: %COPYARTIFACT_BUILD_NUMBER_MEDIASDK_BUILD%&#xd;
echo MEDIASDK_BUILD_NUMBER: %MEDIASDK_BUILD_NUMBER%&#xd;
echo COPYARTIFACT_BUILD_NUMBER_SDKInstallManager_BUILD: %COPYARTIFACT_BUILD_NUMBER_SDKInstallManager_BUILD%&#xd;
&#xd;
if Not &quot;%MEDIASDK_BUILD_NUMBER%&quot;==&quot;%COPYARTIFACT_BUILD_NUMBER_MEDIASDK_BUILD%&quot; (&#xd;
	echo The Build number selected in the MEDIASDK_BUILD_NUMBER drop down does not match with the MediaSDK Build Artifacts copied.&#xd;
	echo Please select the correct MEDIASDK_BUILD_NUMBER&#xd;
	exit /b 1&#xd;
)&#xd;
&#xd;
echo Build numbers match. Proceeding.&#xd;
&#xd;
&quot;%SEVENZ%&quot; x -aoa &quot;MediaSDK-Source-%OSTYPE%-*.7z&quot;&#xd;
if ERRORLEVEL 1 (&#xd;
	exit /b ERRORLEVEL&#xd;
)&#xd;
&#xd;
set MM=&#xd;
if &quot;%P%&quot;==&quot;x86&quot; (&#xd;
	set MM=(VCEDEM_x86.ism VQMFT_WinStore_x86.ism VQMFT_x86.ism)&#xd;
)&#xd;
&#xd;
if &quot;%P%&quot;==&quot;amd64&quot; (&#xd;
	set MM=(VCEDEM_x64.ism VQMFT_WinStore_x64.ism VQMFT_x64.ism)&#xd;
)&#xd;
&#xd;
if &quot;%P%&quot;==&quot;neutral&quot; (&#xd;
	set MM=(DemSources.ism MediaFoundationSources.ism Common.ism StreamData.ism AMFSources.ism)&#xd;
)&#xd;
&#xd;
set MM=%MM%)&#xd;
set MM_DIR=%WORKSPACE%\installers\win\MergeModules&#xd;
&#xd;
echo MM_DIR: %MM_DIR%&#xd;
echo Building Merge Modules: %MM%&#xd;
for %%I in %MM% do (&#xd;
	echo Executing: &quot;%ISCMDBLD%&quot; -a CAS -r GA -v -x -p &quot;%MM_DIR%\%%I&quot; -y %PRODUCT_VERSION%&#xd;
	&quot;%ISCMDBLD%&quot; -a CAS -r GA -v -x -p &quot;%MM_DIR%\%%I&quot; -y %PRODUCT_VERSION%&#xd;
	if ERRORLEVEL 1 (&#xd;
		exit /b ERRORLEVEL &#xd;
	)&#xd;
)&#xd;
&#xd;
if ERRORLEVEL 0 (&#xd;
	echo %P% Merge Modules build successfully, signing the merge modules&#xd;
	call &quot;%ProgramFiles(x86)%\Microsoft Visual Studio 11.0\Common7\Tools\VsDevCmd.bat&quot;&#xd;
&#xd;
	echo -----------------Signing msms.---------------&#xd;
&#xd;
	set MM_DIR=%WORKSPACE%\installers\win\MergeModules&#xd;
	set COPYRIGHT=&quot;Copyright (c), %DATE:~-4%. Advanced Micro Devices, Inc.&quot;&#xd;
&#xd;
	for /r &quot;%MM_DIR%&quot; %%G in (*.msm) do (&#xd;
		&quot;signtool&quot; sign /sm /a &quot;%%~G&quot;&#xd;
		&quot;signtool&quot; timestamp /t http://timestamp.verisign.com/scripts/timestamp.dll &quot;%%~G&quot;&#xd;
	)&#xd;
	&#xd;
	echo ----------------- Done Signing--------------&#xd;
	echo Creating archive...&#xd;
	&#xd;
	&quot;%SEVENZ%&quot; a -t7z MediaSDK-v%ARTIFACT_PRODUCTVERSION%-%OSTYPE%-%P%-MM.7z &quot;.\installers\win\MergeModules\*.msm&quot;&#xd;
)&#xd;
&#xd;
endlocal</command>
    </hudson.tasks.BatchFile>

      <info>
        <groovyScriptContent>def PRODUCT_VERSION=MAJOR_NUMBER + &quot;.&quot; + MINOR_NUMBER + &quot;.&quot; + MEDIASDK_BUILD_NUMBER + &quot;.&quot; + BUILD_NUMBER;
def map = [:];
map.put( &quot;PRODUCT_VERSION&quot;, PRODUCT_VERSION );

def ARTIFACT_PRODUCTVERSION=MAJOR_NUMBER + &quot;.&quot; + MINOR_NUMBER ;
if ( &quot;0&quot; != SUBMINOR_NUMBER ) {
    ARTIFACT_PRODUCTVERSION+=&quot;-&quot; + SUBMINOR_NUMBER;
}

def PRODUCTION_URL_VERSION= ARTIFACT_PRODUCTVERSION + &quot;-&quot; + MILESTONE;
map.put( &quot;PRODUCTION_URL_VERSION&quot;, PRODUCTION_URL_VERSION );

ARTIFACT_PRODUCTVERSION+=&quot;.&quot; + MEDIASDK_BUILD_NUMBER + &quot;.&quot; + BUILD_NUMBER + &quot;-&quot; + MILESTONE;
map.put( &quot;ARTIFACT_PRODUCTVERSION&quot;, ARTIFACT_PRODUCTVERSION );

println &apos;---------------------------------&apos;;
println map;
println &apos;---------------------------------&apos;;

return map;</groovyScriptContent>
        <loadFilesFromMaster>false</loadFilesFromMaster>

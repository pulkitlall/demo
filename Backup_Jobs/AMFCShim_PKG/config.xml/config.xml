        <groovyScriptContent>// Copyright (c) 2015 Advanced Micro Devices, Inc. All rights reserved.

import java.lang.String;

//Dump the environment variables
def map = [:];

map.put( &quot;ARCHIVE_EXTENSION&quot;, &quot;7z&quot; );
map.put( &quot;J&quot;, &quot;1.8&quot; );
map.put( &quot;P&quot;, &quot;x64&quot; );

String strVSName = &quot;VS2013&quot;;
String strVSNameSmall = &quot;vs13&quot;;
String strVSNameCaps = &quot;VS13&quot;; 

if ( JOB_NAME.contains( &quot;v120&quot; ) ) {
	strVSName = &quot;VS2013&quot;;
	strVSNameSmall = &quot;vs12&quot;;
	strVSNameCaps = &quot;VS13&quot;;
	map.put( &quot;VCTOOLKIT_SHORT&quot;, &quot;vc12&quot; );
	map.put( &quot;VSKEY&quot;, &quot;12.0&quot; );
	map.put( &quot;VCNAME_REDIST&quot;, &quot;VC 2013&quot; );
}

if ( JOB_NAME.contains( &quot;v110&quot; ) ) {
	strVSName = &quot;VS2012&quot;;
	strVSNameSmall = &quot;vs11&quot;;
	strVSNameCaps = &quot;VS12&quot;;
	map.put( &quot;VCTOOLKIT_SHORT&quot;, &quot;vc11&quot; );
	map.put( &quot;VSKEY&quot;, &quot;11.0&quot; );
	map.put( &quot;VCNAME_REDIST&quot;, &quot;VC 2012&quot; );
}

if ( JOB_NAME.contains( &quot;v100&quot; ) ) {
	strVSName = &quot;VS2010&quot;;
	strVSNameSmall = &quot;vs10&quot;;	
	strVSNameCaps = &quot;VS10&quot;;
	map.put( &quot;VCTOOLKIT_SHORT&quot;, &quot;vc10&quot; );
	map.put( &quot;VSKEY&quot;, &quot;10.0&quot; );
	map.put( &quot;VCNAME_REDIST&quot;, &quot;VC 2010&quot; );
}

map.put( &quot;VSNAME_SMALL&quot;, strVSNameSmall );
map.put( &quot;VSNAME_CAPS&quot;, strVSNameCaps );
map.put( &quot;VSNAME&quot;, strVSName );

def PRODUCT_VERSION_DISPLAY = MAJOR_NUMBER + &quot;.&quot; + MINOR_NUMBER;
if( &quot;0&quot; != SUBMINOR_NUMBER ) {
	PRODUCT_VERSION_DISPLAY += &quot;-&quot; + SUBMINOR_NUMBER;
}
  
if ( MILESTONE != &quot;PR&quot; || MILESTONE != &quot;GA&quot; ) {
	PRODUCT_VERSION_DISPLAY += &quot; &quot; + MILESTONE;
}

map.put( &quot;PRODUCT_VERSION_DISPLAY&quot;, PRODUCT_VERSION_DISPLAY );

def ARTIFACT_PRODUCTVERSION = MAJOR_NUMBER + &quot;.&quot; + MINOR_NUMBER;
if( &quot;0&quot; != SUBMINOR_NUMBER ) {
	ARTIFACT_PRODUCTVERSION += &quot;-&quot; + SUBMINOR_NUMBER;
}
ARTIFACT_PRODUCTVERSION += &quot;.&quot; + AMFCShim_BUILD_NUMBER + &quot;.&quot; + BUILD_NUMBER + &quot;-&quot; + MILESTONE;

map.put( &quot;ARTIFACT_PRODUCTVERSION&quot;, ARTIFACT_PRODUCTVERSION );

def COMPANYNAME_SMALL=&quot;AMD&quot;;
map.put( &quot;COMPANYNAME_SMALL&quot;, COMPANYNAME_SMALL );
map.put( &quot;COMPANYNAME&quot;, &quot;Advanced Micro Devices&quot; );

def PRODUCT_NAME_DIR=&quot;Media Package&quot;;
map.put( &quot;PRODUCT_NAME_DIR&quot;, PRODUCT_NAME_DIR );
map.put( &quot;PRODUCTNAME&quot;, COMPANYNAME_SMALL + &quot; &quot; + PRODUCT_NAME_DIR );

def PRODUCT_VERSION=MAJOR_NUMBER + &quot;.&quot; + MINOR_NUMBER + &quot;.&quot; + AMFCShim_BUILD_NUMBER + &quot;.&quot; + BUILD_NUMBER;
map.put( &quot;PRODUCT_VERSION&quot;, PRODUCT_VERSION );
map.put( &quot;ARCHIVE_EXTENSION&quot;, &quot;7z&quot; );

println &apos;--------------------------------------------------------------&apos;;
println &apos;JOB_NAME: &apos; + JOB_NAME;
println &apos;BUILD_NUMBER: &apos; + BUILD_NUMBER;
println &apos;Environment variables inserted: &apos; + map;
println &apos;--------------------------------------------------------------&apos;;

return map;</groovyScriptContent>
